    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/rectangle_23"
        android:layout_width="90dp"
        android:layout_height="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginRight="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@drawable/rectangle_23"
        android:layout_marginBottom="10dp">

        <ImageView
            android:id="@+id/iv_write"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="13dp"
            android:layout_marginTop="15dp"
            android:layout_alignParentTop="true"
            app:layout_constraintStart_toStartOf="@+id/rectangle_23"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/ic_floating_plus"/>

        <TextView
            android:id="@+id/tv_write"
            android:layout_width="45dp"
            android:layout_height="17dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="@string/tv_write"
            android:textAppearance="@style/tv_write"
            app:layout_constraintStart_toEndOf="@+id/iv_write"
            app:layout_constraintTop_toTopOf="@+id/iv_write"
            app:layout_constraintBottom_toBottomOf="@+id/iv_write"
            android:gravity="bottom"
            android:layout_marginTop="2dp"
            android:layout_marginStart="5dp"
            />

