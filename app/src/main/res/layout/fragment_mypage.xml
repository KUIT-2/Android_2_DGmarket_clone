<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="13dp"
            android:src="@drawable/ic_mydaangn_setting_outline_24"
            android:text="내 당근"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="129dp"
            android:layout_height="35dp"
            android:layout_marginStart="14dp"
            android:layout_marginTop="60dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.cardview.widget.CardView
                android:id="@+id/iv_user_profile_image"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:backgroundTint="#CCCCCC"
                app:cardCornerRadius="100dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/profile" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_width="83dp"
                android:layout_height="22dp"
                android:layout_marginStart="11dp"
                android:fontFamily="@font/inter_bold"
                android:text="황졍민"
                android:textColor="@color/black"
                android:textSize="18dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/iv_user_profile_image"
                app:layout_constraintTop_toTopOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.cardview.widget.CardView
            android:id="@+id/cv_view_pofile"
            android:layout_width="80dp"
            android:layout_height="29dp"
            android:layout_marginTop="61dp"
            android:layout_marginRight="16dp"
            android:backgroundTint="#F2F3F7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:layout_width="54dp"
                android:layout_height="13dp"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="8dp"
                android:layout_marginRight="13dp"
                android:layout_marginBottom="8dp"
                android:fontFamily="@font/inter_bold"
                android:gravity="center"
                android:text="프로필 보기"
                android:textColor="@color/black"
                android:textSize="11dp" />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cv_carrot_pay"
            android:layout_width="331dp"
            android:layout_height="106dp"
            android:layout_marginTop="114dp"
            android:layout_marginRight="15dp"
            android:layout_marginLeft="14dp"
            app:strokeWidth="3dp"
            android:backgroundTint="#FFFFFF"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_carrot_pay"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="244dp"
                    android:layout_height="16dp"
                    android:layout_marginLeft="32dp"
                    android:layout_marginTop="25dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    >

                    <ImageView
                        android:id="@+id/cl_logo"
                        android:layout_width="56dp"
                        android:layout_height="14dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/onboarding_logo"
                        app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>

                    <TextView
                        android:layout_width="174dp"
                        android:layout_height="16dp"
                        android:fontFamily="@font/inter_regular"
                        android:text="당근하는 새로운 방법, 당근페이!"
                        android:textColor="@color/black"
                        android:textSize="12.5dp"
                        android:layout_marginStart="14dp"
                        app:layout_constraintStart_toEndOf="@+id/cl_logo"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:layout_editor_absoluteX="70dp"
                        android:gravity="center"
                        />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <ImageView
                    android:layout_width="9dp"
                    android:layout_height="15dp"
                    android:layout_marginRight="19dp"
                    android:src="@drawable/icon_post_article_more"
                    android:layout_marginTop="24dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_charge"
                    android:layout_width="143dp"
                    android:layout_height="33dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="@+id/cl_carrot_pay"
                    android:layout_marginStart="18dp"
                    android:layout_marginBottom="15dp"
                    android:backgroundTint="#F2F3F7"
                    >
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <androidx.constraintlayout.widget.ConstraintLayout
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            android:layout_width="42dp"
                            android:layout_height="16dp">

                            <ImageView
                                android:id="@+id/iv_charge_btn"
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:src="@drawable/tab_icon_write_gb_n"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"/>

                            <TextView
                                android:layout_width="24dp"
                                android:layout_height="16dp"
                                android:textColor="@color/black"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toEndOf="@+id/iv_charge_btn"
                                android:autoSizeTextType="uniform"
                                android:layout_marginStart="3dp"
                                android:fontFamily="@font/inter_bold"
                                android:text="충전"/>


                        </androidx.constraintlayout.widget.ConstraintLayout>

                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_account"
                    android:layout_width="143dp"
                    android:layout_height="33dp"
                    app:layout_constraintStart_toEndOf="@id/cv_charge"
                    app:layout_constraintBottom_toBottomOf="@+id/cl_carrot_pay"
                    android:layout_marginStart="9dp"
                    android:layout_marginBottom="15dp"
                    android:backgroundTint="#F2F3F7"
                    >
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <androidx.constraintlayout.widget.ConstraintLayout
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            android:layout_width="42dp"
                            android:layout_height="16dp">

                            <ImageView
                                android:id="@+id/iv_account_btn"
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:src="@drawable/tab_icon_write_gb_n"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"/>

                            <TextView
                                android:layout_width="24dp"
                                android:layout_height="16dp"
                                android:textColor="@color/black"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toEndOf="@+id/iv_account_btn"
                                android:autoSizeTextType="uniform"
                                android:layout_marginStart="3dp"
                                android:fontFamily="@font/inter_bold"
                                android:text="계좌"/>


                        </androidx.constraintlayout.widget.ConstraintLayout>

                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.cardview.widget.CardView>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.cardview.widget.CardView>


        <TextView
            android:id="@+id/tv_recent_visit"
            android:layout_width="55dp"
            android:layout_height="17dp"
            android:text="최근 방문"
            android:textColor="@color/black"
            android:autoSizeTextType="uniform"
            android:layout_marginTop="27dp"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/inter_extrabold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cv_carrot_pay"
            />
        <HorizontalScrollView
            android:id="@+id/sv_recent_visit"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_recent_visit"
            android:layout_marginTop="19dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="68dp"
                    android:layout_height="33dp"
                    android:layout_marginStart="15dp"
                    android:background="@drawable/btn_round_shape"
                    android:backgroundTint="@color/white"
                    android:drawableLeft="@drawable/btn_entire"
                    android:text="전체"
                    android:autoSizeTextType="uniform"
                    android:textColor="@color/black"
                    android:paddingLeft="12dp"
                    android:paddingRight="4dp"
                    android:paddingTop="8dp"
                    android:paddingBottom="8dp"
                    android:textSize="14dp"
                    >

                </androidx.appcompat.widget.AppCompatButton>

                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="99dp"
                    android:layout_height="33dp"
                    android:layout_marginStart="7dp"
                    android:background="@drawable/btn_round_shape"
                    android:backgroundTint="@color/white"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp"
                    android:paddingTop="8dp"
                    android:paddingBottom="8dp"
                    android:autoSizeTextType="uniform"
                    android:text="남성패션/잡화">

                </androidx.appcompat.widget.AppCompatButton>

                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="73dp"
                    android:layout_height="33dp"
                    android:layout_marginStart="7dp"
                    android:background="@drawable/btn_round_shape"
                    android:backgroundTint="@color/white"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp"
                    android:paddingTop="8dp"
                    android:paddingBottom="8dp"
                    android:autoSizeTextType="uniform"
                    android:text="여성의류">

                </androidx.appcompat.widget.AppCompatButton>

                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="99dp"
                    android:layout_height="33dp"
                    android:layout_marginStart="7dp"
                    android:background="@drawable/btn_round_shape"
                    android:backgroundTint="@color/white"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp"
                    android:paddingTop="8dp"
                    android:paddingBottom="8dp"
                    android:autoSizeTextType="uniform"
                    android:text="가구/인테리어">

                </androidx.appcompat.widget.AppCompatButton>

                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="60dp"
                    android:layout_height="33dp"
                    android:layout_marginStart="7dp"
                    android:background="@drawable/btn_round_shape"
                    android:backgroundTint="@color/white"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp"
                    android:paddingTop="8dp"
                    android:paddingBottom="8dp"
                    android:autoSizeTextType="uniform"
                    android:text="생필품">

                </androidx.appcompat.widget.AppCompatButton>

            </LinearLayout>

        </HorizontalScrollView>

        <TextView
            android:id="@+id/tv_my_deal"
            android:layout_width="55dp"
            android:layout_height="17dp"
            android:text="나의 거래"
            android:textColor="@color/black"
            android:autoSizeTextType="uniform"
            android:layout_marginTop="27dp"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/inter_extrabold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/sv_recent_visit"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_like_list"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_my_deal"
            android:layout_marginTop="12dp"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="85dp"
                android:layout_height="18dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="16dp">

                <ImageView
                    android:id="@+id/iv_like_list"
                    android:layout_width="21dp"
                    android:layout_height="18dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:src="@drawable/ic_feed_interest_outline_18"/>

                <TextView
                    android:layout_width="52dp"
                    android:layout_height="17dp"
                    android:text="관심목록"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:fontFamily="@font/inter_semibold"
                    android:autoSizeTextType="uniform"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_sell_list"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cl_like_list"
            android:layout_marginTop="12dp"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="85dp"
                android:layout_height="18dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="16dp">

                <ImageView
                    android:id="@+id/iv_sell_list"
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:src="@drawable/ic_community_outline_m"/>

                <TextView
                    android:layout_width="52dp"
                    android:layout_height="17dp"
                    android:text="판매내역"
                    android:textColor="@color/black"
                    android:fontFamily="@font/inter_semibold"
                    android:autoSizeTextType="uniform"
                    android:gravity="center"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_buy_list"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cl_sell_list"
            android:layout_marginTop="12dp"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="85dp"
                android:layout_height="18dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="16dp">

                <ImageView
                    android:id="@+id/iv_buy_list"
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:src="@drawable/ic_voucher_outline_m"/>

                <TextView
                    android:layout_width="52dp"
                    android:layout_height="17dp"
                    android:text="구매내역"
                    android:textColor="@color/black"
                    android:fontFamily="@font/inter_semibold"
                    android:autoSizeTextType="uniform"
                    android:gravity="center"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>



            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_account_book"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cl_buy_list"
            android:layout_marginTop="12dp"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="128dp"
                android:layout_height="18dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="16dp">

                <ImageView
                    android:id="@+id/iv_account_book"
                    android:layout_width="21dp"
                    android:layout_height="18dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:src="@drawable/btn_favorite_text_for_post"/>

                <TextView
                    android:layout_width="95dp"
                    android:layout_height="17dp"
                    android:text="중고거래 가계부"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:fontFamily="@font/inter_semibold"
                    android:autoSizeTextType="uniform"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <View
            android:id="@+id/mypage_line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#F3F3F5"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/cl_account_book" />

        <TextView
            android:id="@+id/tv_my_life"
            android:layout_width="81dp"
            android:layout_height="17dp"
            android:text="나의 동네생활"
            android:textColor="@color/black"
            android:autoSizeTextType="uniform"
            android:layout_marginTop="20dp"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/inter_extrabold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mypage_line"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_my_life"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_my_life"
            android:layout_marginTop="12dp"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="133dp"
                android:layout_height="18dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="16dp">

                <ImageView
                    android:id="@+id/iv_my_life"
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:src="@drawable/ic_mydaangn_storyarticle_outline_24"/>

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="17dp"
                    android:text="동네생활 글/댓글"
                    android:gravity="center"
                    android:fontFamily="@font/inter_semibold"
                    android:autoSizeTextType="uniform"
                    android:textColor="@color/black"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <View
            android:id="@+id/mypage_line2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#F3F3F5"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/cl_my_life" />

        <TextView
            android:id="@+id/tv_part_mange"
            android:layout_width="81dp"
            android:layout_height="17dp"
            android:text="알바 관리"
            android:textColor="@color/black"
            android:autoSizeTextType="uniform"
            android:layout_marginTop="20dp"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/inter_extrabold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mypage_line2"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_apply_list"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_part_mange"
            android:layout_marginTop="12dp"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="85dp"
                android:layout_height="18dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="16dp">

                <ImageView
                    android:id="@+id/iv_apply_list"
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:src="@drawable/ic_mydaangn_storyarticle_outline_24"/>

                <TextView
                    android:layout_width="52dp"
                    android:layout_height="17dp"
                    android:text="지원내역"
                    android:gravity="center"
                    android:fontFamily="@font/inter_semibold"
                    android:autoSizeTextType="uniform"
                    android:textColor="@color/black"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <View
            android:id="@+id/mypage_line3"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#F3F3F5"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/cl_apply_list" />

        <TextView
